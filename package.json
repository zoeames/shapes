  "directories": {
    "test": "test"
  },
  "bin": {
    "coveralls": "./node_modules/coveralls/bin/coveralls.js"
  },
  "scripts": {
    "start": "node server/index.js",
    "postinstall": "grunt deploy",
    "test": "mocha test --recursive -R spec --timeout 5000",
