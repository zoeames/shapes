    "chalk": "^0.5.1",
    "connect-redis": "^2.2.0",
    "coveralls": "^2.11.2",
    "express": "^4.11.1",
    "express-method-override": "0.0.3",
    "express-session": "^1.10.1",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-copy": "^0.7.0",
