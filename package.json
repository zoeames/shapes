{
  "name": "Shapes",
  "version": "0.0.0",
  "description": "A 3D exploration.",
  "engines": {
    "node": "0.10.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zoeames/shapes"
  },
